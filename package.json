{
    "name": "DancingBaby",
    "version": "1.0.0",
    "description": "Fair Usage Assessment Tool",
    "main": "index.js",
    "scripts": {
        "build": "webpack --config ./webpack.config.js",
        "start": "node ./server/server.js",
        "test": "NODE_ENV=test jest --coverage",
        "dev:hot": "NODE_ENV=development nodemon ./server/server.js & webpack-dev-server --open --hot --inline --progress --colors --watch --content-base ./",
        "add-dep": "bash ./scripts/npm-docker/add-dep.sh",
        "update-dep": "bash ./scripts/npm-docker/update-dep.sh",
        "docker-remove-all:danger": "docker ps -a -q -f 'name=db-' | xargs docker rm --force && echo 'Removed all containers.' && docker images dbsites/db-* -q | xargs docker rmi --force && echo 'Removed all images.' && docker volume ls -q -f name=dancingbaby | xargs docker volume rm --force && echo 'Removed all volumes.' && echo 'REMOVE ALL COMPLETE'",
        "docker-stop": "docker ps -a -q --filter status=running | xargs docker stop && echo 'Stopped all running containers.'",
        "docker-dev:hot": "docker-compose -f docker-compose-dev-hot.yml up"
    },
    "author": "Schno Mozingo and Neil Katz",
    "license": "MIT",
    "dependencies": {
        "bcrypt": "^3.0.6",
        "buffer-shims": "^1.0.0",
        "config": "^3.1.0",
        "connect-pg-simple": "^5.0.0",
        "cookie-parser": "^1.4.4",
        "express-session": "^1.16.1",
        "flash": "^1.1.0",
        "gasp": "0.0.2",
        "gsap": "^2.1.3",
        "html-loader": "^0.5.5",
        "moment": "^2.24.0",
        "moment-timezone": "^0.5.25",
        "multer": "^1.4.1",
        "node-fetch": "^2.6.0",
        "passport": "^0.4.0",
        "passport-local": "^1.0.0",
        "pg": "^7.7.1",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-redux": "^5.0.6",
        "redux": "^3.7.2",
        "redux-form": "^7.2.0",
        "redux-thunk": "^2.3.0",
        "req-flash": "0.0.3",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.4",
        "css-loader": "^1.0.1",
        "cssloader": "^1.1.1",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.1.0",
        "morgan": "^1.9.1",
        "node-sass": "^4.12.0",
        "nodemon": "^1.18.9",
        "react-hot-loader": "^4.6.3",
        "redux-devtools-extension": "^2.13.5",
        "ret": "^0.2.2",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "url-loader": "^1.1.2",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.1.9",
        "webpack-hot-middleware": "^2.24.3"
    }
}
